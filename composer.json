{
    "name": "tiknil/skipper",
    "description": "CLI application to manage multiple instances of local web applications",
    "keywords": [ "laravel", "docker", "cli" ],
    "type": "library",
    "license": "MIT",
    "version": "0.2.0",
    "bin": [
        "bin/skipper"
    ],
    "authors": [
        {
            "name": "Giorgio Balduzzi",
            "email": "balduzzi.giorgio@tiknil.com"
        }
    ],
    "require": {
        "ext-posix": "*",
        "php": "^8.0",
        "symfony/console": "^6.2",
        "symfony/process": "^6.2",
        "symfony/yaml": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Tiknil\\Skipper\\": "src/"
        },
        "files": ["src/Helpers.php"]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/pint": "^1.10"
    }
}
